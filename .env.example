# Пример файла конфигурации окружения
# Скопируйте этот файл в .env и заполните своими значениями

# API ключи
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# Альфа-Инвестиции API
ALFA_TOKEN=your_alfa_invest_token_here
ALFA_ACCOUNT_ID=your_account_id_here

# Настройки торговли
DEFAULT_SYMBOL=SBER
INITIAL_BALANCE=100000
MAX_POSITION_SIZE=0.1
USE_ALFA_BROKER=true

# Настройки модели машинного обучения
PREDICTION_WINDOW=30
TRAINING_PERIOD=252

# Настройки логирования
LOG_LEVEL=INFO
LOG_FILE=logs/trading_bot.log